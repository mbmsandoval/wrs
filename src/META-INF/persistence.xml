<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="wrs">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>data.entities.Country</class>
		<class>data.entities.Source</class>
		<class>data.entities.Variable</class>
		<class>data.entities.VariablesWithValueFromSource</class>
		
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/wrsdb" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="1rr1" />
			<property name="eclipselink.ddl-generation.output-mode" value="both" />
		</properties>
		
	</persistence-unit>
</persistence>
